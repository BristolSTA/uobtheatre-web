{
  "name": "uobtheatre",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.ts,.vue,.graphql\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "lint": "yarn lint:js && yarn lint:style",
    "test:unit": "jest",
    "pretest:e2e": "yarn generate --devtools && yarn start",
    "pretest:e2e:local": "API_BASE=http://uobtheatre-api:8000 yarn generate --devtools && yarn start",
    "pretest:e2e:ci": "API_BASE=http://localhost:9000 yarn generate --devtools && yarn start",
    "test:e2e:silent": "cypress run",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/apollo": "^4.0.1-rc.5",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/sentry": "^5.1.4",
    "@tailwindcss/forms": "^0.3.3",
    "@tiptap/starter-kit": "^2.0.0-beta.89",
    "@tiptap/vue-2": "^2.0.0-beta.42",
    "@vue/composition-api": "^1.1.1",
    "axios": "^0.24.0",
    "core-js": "^3.15.2",
    "humanize-duration": "^3.27.0",
    "js-cookie": "^2.2.1",
    "jwt-decode": "^3.1.2",
    "leaflet": "^1.7.1",
    "lodash": "^4.17.20",
    "luxon": "^2.0.1",
    "nuxt": "^2.15.7",
    "qrcode.vue": "^1.7.0",
    "sweetalert2": "^11.0.20",
    "uuid": "^8.3.2",
    "v-money": "^0.8.1",
    "vue-currency-input": "^2.0.2",
    "vue-gtag": "^1.16.1",
    "vue-qrcode-reader": "^3.0.0",
    "vue-tailwind": "^2.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^12.1.4",
    "@cypress/vue": "^2.2.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/vue-fontawesome": "^2.0.0",
    "@graphql-codegen/cli": "2.13.7",
    "@graphql-codegen/client-preset": "1.1.1",
    "@graphql-codegen/gql-tag-operations-preset": "^1.7.0",
    "@graphql-codegen/typescript-vue-apollo-smart-ops": "^2.3.5",
    "@graphql-eslint/eslint-plugin": "^3.12.0",
    "@nuxt/test-utils": "^3.0.0-rc.12",
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^11.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/fontawesome": "^1.1.2",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@nuxtjs/tailwindcss": "^4.2.1",
    "@types/lodash": "^4.14.186",
