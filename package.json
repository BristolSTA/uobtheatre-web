    "cypress": "^7.6.0",
    "cypress-nuxt": "^1.3.1",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-jsdoc": "^35.5.1",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.14.0",
    "faker": "^5.5.3",
    "got": "^11.8.5",
    "graphql-tag": "^2.12.5",
    "husky": "^7.0.1",
